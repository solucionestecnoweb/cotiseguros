<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_islr_retention_supplier" model="ir.sequence">
            <field name="name">ISLR Retention supplier</field>
            <field name="code">islr.retention.supplier</field>
            <field name="implementation">standard</field>
            <field name="prefix">%(year)s%(month)s</field>
            <field name="padding">8</field>
            <field name="active" eval="1"/>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_islr_retention_customer" model="ir.sequence">
            <field name="name">ISLR Retention customer</field>
            <field name="code">islr.retention.customer</field>
            <field name="implementation">standard</field>
            <field name="prefix">%(year)s%(month)s</field>
            <field name="padding">8</field>
            <field name="active" eval="1"/>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_isrl_retention_entry" model="ir.sequence">
            <field name="name">ISLR Retention Account</field>
            <field name="code">islr.retention.account</field>
            <field name="implementation">standard</field>
            <field name="prefix">ISRL/%(year)s%(month)s</field>
            <field name="padding">8</field>
            <field name="active" eval="1"/>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>